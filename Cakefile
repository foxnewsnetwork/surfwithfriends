fs = require 'fs'

task 'clusterfuck', 'takes everything in the public/javascripts folder and merges into 1 file' ->
	clusterfuck = (file) ->
		fs.open(file, "r", () )
		fs.appendFileSync( "./output/clusterfuck.js", 
	// clusterfuck
	fs.readdir "./public/javascripts/", (files) ->  
		clusterfuck file for file in files		
	// fs.readdir
